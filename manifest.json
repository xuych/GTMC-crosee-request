{
    "manifest_version": 2,
    "name": "GTMC 请求代理插件",
    "description": "GTMC API管理系统 请求代理谷歌插件",
    "version": "3.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["webRequest", "webRequestBlocking"],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "web_accessible_resources": [
        "index.js"
    ],
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": [
            "response.js"
        ],
        "all_frames": true
    }]
}